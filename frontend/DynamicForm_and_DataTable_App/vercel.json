{
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://medicdata-store.onrender.com/api/$1"
      },
      {
        "source": "/uploads/(.*)",
        "destination": "https://medicdata-store.onrender.com/uploads/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }
  